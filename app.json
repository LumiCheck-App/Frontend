{
  "expo": {
    "name": "lumicheck",
    "slug": "lumicheck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lumis/Lumi4k.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/lumis/Lumi4k.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserTrackingUsageDescription": "Este app utiliza notificações para manter você informado sobre atualizações importantes.",
        "UIUserNotificationSettings": {
          "UIUserNotificationTypeAlert": true,
          "UIUserNotificationTypeBadge": true,
          "UIUserNotificationTypeSound": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lumis/Lumi4k.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/lumis/Lumi4k.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}